{
  "funnel_name": "sample_funnel",
  "project_id": "your-project-id",
  "dataset": "your_dataset",
  "data_source": "your_events_table",
  "user_base_table": "your_users_table",
  "time_period_days": 30,
  "steps": [
    {
      "rank": 1,
      "screen_name": "Home Page",
      "loaded_events": ["page_view"],
      "filters": {
        "page_path": "/home"
      }
    },
    {
      "rank": 2,
      "screen_name": "Product Browse",
      "loaded_events": ["page_view", "click"],
      "filters": {
        "page_path": "/products"
      }
    },
    {
      "rank": 3,
      "screen_name": "Product Detail",
      "loaded_events": ["page_view", "click"],
      "filters": {
        "page_path": "/product/"
      }
    },
    {
      "rank": 4,
      "screen_name": "Add to Cart",
      "loaded_events": ["click", "add_to_cart"],
      "filters": {
        "event_name": "add_to_cart"
      }
    },
    {
      "rank": 5,
      "screen_name": "Checkout",
      "loaded_events": ["page_view", "begin_checkout"],
      "filters": {
        "page_path": "/checkout"
      }
    },
    {
      "rank": 6,
      "screen_name": "Purchase",
      "loaded_events": ["purchase"],
      "filters": {
        "event_name": "purchase"
      }
    }
  ],
  "filters": {
    "country": "US",
    "device_category": "mobile"
  },
  "user_id_column": "user_id",
  "session_id_column": "session_id",
  "event_column": "event_name",
  "timestamp_column": "event_timestamp",
  "partition_column": "event_date",
  "user_segmentation_fields": ["country", "device_category", "user_type"],
  "max_step": 6
} 